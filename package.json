{
  "name": "@f8n-ai/semdag",
  "private": false,
  "homepage": "https://github.com/f8n-ai/semdat",
  "author": "Eugene Yaroslavtsev <eugene@f8n.ai>",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && bun build ./index.ts --minify --splitting --outdir=dist --target=node --format=esm && bun build ./index.ts --minify --splitting --outdir=dist --target=node --format=cjs",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "check": "biome check ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "lefthook": "^1.10.10",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "trustedDependencies": ["@biomejs/biome", "lefthook"],
  "engines": {
    "bun": "1.2.2"
  }
}
